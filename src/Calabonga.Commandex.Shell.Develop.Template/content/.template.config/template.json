{
    "$schema": "http://json.schemastore.org/template",
    "author": "Calabonga SOFT",
    "classifications": [
        "Windows",
        "Desktop",
        "C#"
    ],
    "name": "Commandex.Shell.Developer for WPF AppDefinitions",
    "description": "Define your Web API application parts in different definitions (files) for NET8.0. It's remarkably simple and useful. You will know what features your application consists of.",
    "identity": "Calabonga.AppDefinitions.Shell",
    "shortName": "wpfshell",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "sourceName": "Calabonga.AppDefinitions.Shell",
    "preferNameDirectory": true,
    "symbols": {
        "Framework": {
            "type": "parameter",
            "description": "The target framework for the project.",
            "datatype": "choice",
            "choices": [
                {
                    "choice": "net8.0"
                }
            ],
            "defaultValue": "net8.0"
        }
    },
    "sources": [
        {
            "modifiers": [
                {
                    "exclude": [
                        ".vs/**",
                        ".idea/**",
                        "_ReSharper.Caches/**",
                        ".template_config/**"
                    ]
                }
            ]
        }
    ]
}