{
  "$schema": "http://json.schemastore.org/template",
  "author": "Calabonga SOFT",
  "classifications": [
    "Common",
    "Windows",
    "Desktop",
    "Templating"
  ],
  "name": "WPF Shell Application for Commandex",
  "description": "Template for WPF Application with CommunityToolkit.Mvvm installed. This application is a simple Commandex Shell emulation that helps developers create a CommandexCommand components.",
  "identity": "Calabonga.Commandex.Shell.Develop",
  "shortName": "wpfshell",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "defaultName": "Commandex.Developer.Shell1",
  "sourceName": "Calabonga.Commandex.Shell.Develop",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net9.0"
        }
      ],
      "defaultValue": "net9.0"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            ".vs/**",
            ".idea/**",
            "_ReSharper.Caches/**",
            ".template_config/**"
          ]
        }
      ]
    }
  ]
}